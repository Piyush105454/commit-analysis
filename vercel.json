{
    "rewrites": [
        {
            "source": "/api/py/:path*",
            "destination": "/api/flask.py"
        },
        {
            "source": "/openapi.json",
            "destination": "/api/flask.py"
        },
        {
            "source": "/docs",
            "destination": "/api/flask.py"
        },
        {
            "source": "/redoc",
            "destination": "/api/flask.py"
        }
    ]
}